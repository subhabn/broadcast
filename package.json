{
	"name": "playground",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --open",
		"build": "ng build runestone --prod && run-p build:post:*",
		"build:post:copyAssets": "cpx  ./src/assets/** ./dist/runestone/assets --include-empty-dirs",
		"build:post:copyNpmrc": "cpx  ./.npmrc ./dist/runestone/",
		"test": "ng test runestone --code-coverage --watch=false",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"publish": "(cd dist/runestone && npm publish)"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^6.0.0",
		"@angular/common": "^6.0.0",
		"@angular/compiler": "^6.0.0",
		"@angular/core": "^6.0.0",
		"@angular/forms": "^6.0.0",
		"@angular/http": "^6.0.0",
		"@angular/platform-browser": "^6.0.0",
		"@angular/platform-browser-dynamic": "^6.0.0",
		"@angular/router": "^6.0.0",
		"@types/adal-angular": "1.0.1",
		"adal-angular": "1.0.17",
		"core-js": "^2.5.4",
		"ng2-semantic-ui": "^0.9.7",
		"rxjs": "^6.0.0",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.6.1",
		"@angular-devkit/build-ng-packagr": "~0.6.1",
		"@angular/cli": "~6.0.1",
		"@angular/compiler-cli": "^6.0.0",
		"@angular/language-service": "^6.0.0",
		"@types/jasmine": "~2.8.6",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"codelyzer": "~4.2.1",
		"cpx": "^1.5.0",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "^2.0.2",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~1.4.2",
		"karma-jasmine": "~1.1.1",
		"karma-jasmine-html-reporter": "^0.2.2",
		"ng-packagr": "^3.0.1",
		"npm-run-all": "4.1.3",
		"protractor": "~5.3.0",
		"ts-node": "~5.0.1",
		"tsickle": ">=0.25.5",
		"tslib": "^1.7.1",
		"tslint": "~5.9.1",
		"tslint-eslint-rules": "^5.2.0",
		"typescript": "~2.7.2"
	}
}
